<dvi class="row">
	<div class="button-group">
		<%= link_to " Back to all Posts", posts_path, class: "button hollow fi-arrow-left"%>
		<%= link_to " Add a new Post", new_post_path, class: "button hollow fi-plus" %>
		</div>
</dvi>

<h4>All users:</h4>
<br>


<% @users.each do |u| %>
	<p>
		<%= link_to "#{u.username}", user_path(u) %>
		<% if u.admin? %>
			Is admin.
		<% else %>
			Not admin
		<% end %>
	</p>
	<hr>
<% end %>